<div class="encabezado">
  <p class = "tituloNodos"><%= link_to "AiRescue", root_path, style: 'color:#FFFFFF;', class: "tituloNodos"%></p>
</div>

<div class = "fondoNodos2">
</div>
<div class = "fondoNodos3">
</div>

<div class= "cuadro">
  <div class="col-xs-8 col-md-6"
    <div class="container">
      <div class="">
      	<div class="panel-heading">
      		<div class="panel panel-default">
  			  	<div class="panel-heading">
                <h2 class="panel-title titulo">AiRescue</h2>
            </div>
            <div class="panel-body">
              <fieldset>
                <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                  <div class="form-group" >
                    <%= f.email_field :email, autofocus: true, class:"form-control", placeholder:"Correo" %>
                  </div>
                  <div class="form-group">
                    <%= f.password_field :password, autocomplete: "off", class:"form-control", placeholder:"ContraseÃ±a" %>
                  </div>
                  <% if devise_mapping.rememberable? -%>
                    <div class = "form-group checkbox check">
                      <%= f.check_box :remember_me %>
                      <%= f.label :remember_me %>
                    </div>   
                  <% end -%>
                  <br />
                  <div class="actions">
                    <%= f.submit "Log in",class:"form-control btn  btn-success" %>
                  </div>
                <% end %>
                <!--<%= render "devise/shared/links" %>-->
              </fieldset>
            </div>
          </div>
        </div>
      </div>  
    </div>
  </div>  
</div>  		
  	
  	

