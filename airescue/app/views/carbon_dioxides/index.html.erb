<p id="notice"><%= notice %></p>

<h1>Carbon Dioxides</h1>

<table>
  <thead>
    <tr>
      <th>Datadioxide</th>
      <th>Node</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carbon_dioxides.each do |carbon_dioxide| %>
      <tr>
        <td><%= carbon_dioxide.dataDioxide %></td>
        <td><%= carbon_dioxide.node %></td>
        <td><%= link_to 'Show', carbon_dioxide %></td>
        <td><%= link_to 'Edit', edit_carbon_dioxide_path(carbon_dioxide) %></td>
        <td><%= link_to 'Destroy', carbon_dioxide, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carbon Dioxide', new_carbon_dioxide_path %>
